    "class Queue:\n",
    "    def __init__(self, head=None):\n",
    "        self.list = [head]\n",
    "        \n",
    "    def enqueue(self, new_element):\n",
    "        self.list.append(new_element)\n",
    "        \n",
    "    def peek(self):\n",
    "        return self.list[0]\n",
    "    \n",
    "    def dequeue(self):\n",
    "        return self.list.pop(0)"
   ]
  }
